{
  "name": "@0x666c6f/sqd-portal-rpc-wrapper",
  "version": "0.1.0",
  "private": true,
  "description": "Standalone SQD Portal JSON-RPC wrapper service",
  "license": "MIT",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc -p tsconfig.build.json",
